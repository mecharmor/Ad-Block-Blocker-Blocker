{
  "manifest_version": 2,

  "name": "Ad Block Blocker Blocker",
  "description": "This extension blocks the ad block blocker",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },

    "content_scripts": [{
      "js": ["content.js"],
      "matches": ["https://www.yahoo.com/*"]
      }],

  "permissions": [
   "activeTab"
   ]
}
